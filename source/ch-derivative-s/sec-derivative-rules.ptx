<subsection xml:id="ssec-derivative-rules-tf">
  <title>Practice: True/False</title>

  <exercises xml:id="exercises-derivative-rules-tf">
    <exercisegroup>
      <title>True/False Questions</title>

      <introduction>
        <p>
          Determine whether each statement is true or false.
          Justify your answer using derivative rules or interpretation.
        </p>


      </introduction>

      <!-- Notation and meaning -->

      <exercise xml:id="tf-deriv-rules-01">
        <statement>
          <p>
            The symbols <m>f'(x)</m> and <m>\displaystyle \frac{df}{dx}</m>
            always represent the same quantity.
          </p>
        </statement>
      </exercise>



      <!-- Basic derivatives -->

      <exercise xml:id="tf-deriv-rules-04">
        <statement>
          <p>
            If <m>f(x)=7^\pi</m>, then <m>f'(x)=0</m> for all <m>x</m>.
          </p>
        </statement>
      </exercise>

      <exercise xml:id="tf-deriv-rules-05">
        <statement>
          <p>
            If <m>f(x)=kx</m>, then <m>f'(x)=k</m>.
          </p>
        </statement>
      </exercise>

      <exercise xml:id="tf-deriv-rules-06">
        <statement>
          <p>
            If <m>f(x)=\dfrac{1}{\sqrt[3]{x^2}}</m>, then <m>f'(x)=-\tfrac23 x^{-5/3}</m>.
          </p>
        </statement>
      </exercise>





      <!-- Product and quotient rules -->

      <exercise xml:id="tf-deriv-rules-11">
        <statement>
          <p>
            If <m>f'(2)=3</m> and <m>h(x)=x^2f(x)</m> then <m>h'(2)=12</m>.
          </p>
        </statement>
      </exercise>



      <exercise xml:id="tf-deriv-rules-14">
        <statement>
          <p>
            It is always valid to simplify a quotient algebraically
            before differentiating.
          </p>
        </statement>
      </exercise>

   



      <!-- Higher derivatives -->

      <exercise xml:id="tf-deriv-rules-18">
        <statement>
          <p>
            If <m>V(x)</m> represents velocity,
            then <m>V'(x)</m> represents acceleration.
          </p>
        </statement>
      </exercise>



      <exercise xml:id="tf-deriv-rules-20">
        <statement>
          <p>
            The notation <m>f'''(x)</m> and <m>f^{(3)}(x)</m>
            represent the same function.
          </p>
        </statement>
      </exercise>

    </exercisegroup>





